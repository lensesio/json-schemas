{
  "name": "lenses-iam-autocomplete",
  "displayName": "Lenses IAM Autocomplete",
  "description": "Autocompletion for IAM roles in YAML files using Lenses backend.",
  "version": "0.0.1",
  "publisher": "lenses",
  "engines": {
    "vscode": ">=1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:yaml"
  ],
  "main": "./dist/extension.js",
  "files": [
    "dist/**/*",
    "schemas/**/*"
  ],
  "contributes": {
    "languages": [
      {
        "id": "yaml",
        "aliases": [
          "YAML",
          "yaml"
        ],
        "extensions": [
          ".yaml",
          ".yml"
        ]
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Lenses",
      "properties": {
        "lenses.baseUrl": {
          "type": "string",
          "default": "http://localhost:9991",
          "description": "The base URL for the Lenses API (e.g., http://localhost:9991). "
        },
        "lenses.apiToken": {
          "type": "string",
          "default": "",
          "description": "The API token for authenticating with the Lenses API."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0",
    "@types/vscode": "^1.70.0"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  }
}
